 note_font Verdana
 note_font_color 0xff
 mapsize {0.15 0.15}
 addUserKnob {20 Overlays}
 addUserKnob {26 Meta}
 addUserKnob {1 MetaFilename_ctrl l Filename t "Defaults to the nuke script name."}
 MetaFilename_ctrl "\[file rootname \[file tail \[value root.name]]]"
 addUserKnob {1 MetaDescription_ctrl l Description t "Add short info about the shot. This text is centred."}
 addUserKnob {6 MetaToggle_ctrl l "Show Meta" t "Check to display shot meta inforamtion." +STARTLINE}
 MetaToggle_ctrl true
 addUserKnob {4 MetaPositionY_ctrl l Position -STARTLINE M {bottom top "title safe" ""}}
 MetaPositionY_ctrl "title safe"
 addUserKnob {26 Rulers}
 addUserKnob {3 RulersDivisions_ctrl l Divisions t "Must be an integer between 2 and 16"}
 RulersDivisions_ctrl 12
 addUserKnob {7 RulersOpacity_ctrl l Opacity}
 RulersOpacity_ctrl 0.75
 addUserKnob {6 RulersToggle_ctrl l "Show Rulers" +STARTLINE}
 RulersToggle_ctrl true
 addUserKnob {6 RulersCentreToggle_ctrl l "Show Centre" -STARTLINE}
 RulersCentreToggle_ctrl true
 addUserKnob {26 Letterbox}
 addUserKnob {4 LetterboxFormat_ctrl l Format M {1.78 1.85 2.35 2.39 2.66 2.76 4 "" ""}}
 LetterboxFormat_ctrl 2.35
 addUserKnob {7 LetterboxOpacity_ctrl l Opacity}
 LetterboxOpacity_ctrl 0.9
 addUserKnob {6 LetterboxToggle_ctrl l "Show Letterbox" +STARTLINE}
 LetterboxToggle_ctrl true
 addUserKnob {26 ""}
 addUserKnob {3 SlateVisibleFrame_ctrl l "Slate Frame" t "The overlays are hidden on this frame."}
 SlateVisibleFrame_ctrl 1040
 addUserKnob {20 Setup}
 addUserKnob {2 Font_ctrl l Font}
 Font_ctrl "\[getenv NUKE_PATH]/fonts/FreeSans.ttf"
 addUserKnob {26 ""}
 addUserKnob {26 Author -STARTLINE T "Richard Greenwood"}
